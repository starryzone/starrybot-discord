{
  "name": "farewell",
  "description": "Kick starrybot itself from your guild",

  "adminOnly": true,
  "ephemeral": true,
  "prompt": {
    "type": "button",
    "title": "This will delete roles created by starrybot.",
    "options": [
      {
        "next": "farewellConfirmation",
        "label": "I understand"
      },
      {
        "next": "farewellRejection",
        "label": "Cancel",
        "style": "Secondary"
      }
    ]
  },
  "steps": {
    "farewellConfirmation": {
      "action": "leaveGuild"
    },
    "farewellRejection": {
      "message": "✨ 👍 🌟",
      "ephemeral": true
    }
  }
}